{
  "name": "wasacommunity",
  "version": "1.0.0",
  "description": "An application made for Wasa Sweden AB",
  "author": {
    "name": "Isak Vidinghoff",
    "email": "isak.vidinghoff@gmail.com",
    "url": "https://www.isakvidinghoff.com"
  },
  "homepage": "https://www.wasacommunity.com",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -k \"ng serve --extract-css\" \"npm run theme\"",
    "theme": "node-sass -w src/app/assets/themes/ -o src/assets/themes/ --output-style=compressed",
    "build": "ng build  --extract-css",
    "test": "karma start ./karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "lint": "ng lint",
    "hmr": "ng serve --configuration hmr",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/cdk": "^5.2.1",
