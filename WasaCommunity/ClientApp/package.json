    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@ngx-translate/core": "^9.1.1",
    "chart.js": "^2.7.1",
    "core-js": "^2.5.3",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.12.0",
    "ng2-charts": "^1.6.0",
